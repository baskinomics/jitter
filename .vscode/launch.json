{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "stopOnEntry": false,
            "mainClass": "jitter.JitterCommand",
            "args": [
                "-c",
                "/home/zoo/development/git/config.yaml"
            ],
            "shortenCommandLine": "auto",
        },
        {
            "type": "java",
            "name": "Debug - Verbose",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "stopOnEntry": false,
            "mainClass": "jitter.JitterCommand",
            "args": [
                "-vv",
                "-c",
                "/home/zoo/development/git/config.yaml"
            ],
            "shortenCommandLine": "auto",
        }
    ]
}